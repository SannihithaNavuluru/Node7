x <-cars$speed
y <- cars$dist
# Plot with main and axis titles
# Change point shape (pch = 19) and remove frame.
plot(x, y, main = "Regression line", xlab = "Speed",
ylab = "Distance",
pch = 20, frame = TRUE)
# Add regression line
reg=lm(y ~ x, data = cars)
reg
abline(reg, col = "blue")
summary(reg)
